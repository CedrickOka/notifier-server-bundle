{
	"name" : "coka/notifier-server-bundle",
	"type" : "symfony-bundle",
	"description" : "Notifier Server Bundle for send notification on many channels",
	"homepage" : "https://github.com/CedrickOka/notifier-server-bundle",
	"keywords" : [
		"notification",
		"REST API",
		"messenger"
	],
	"license" : "MIT",
	"autoload" : {
		"psr-4" : {
			"Oka\\Notifier\\ServerBundle\\" : "src"
		},
		"files" : [
			"vendor/onlinecity/php-smpp/autoload.php"
		],
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\Notifier\\ServerBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.2",
		"coka/input-handler-bundle" : "^1.2",
		"coka/notifier-message" : "^1.0",
		"symfony/config" : "4.4.*",
		"symfony/console" : "4.4.*",
		"symfony/dependency-injection" : "4.4.*",
		"symfony/messenger" : "4.4.*",
		"symfony/serializer-pack" : "^1.0",
		"symfony/yaml" : "4.4.*"
	},
	"require-dev" : {
		"coka/pagination-bundle" : "^5.8",
		"doctrine/mongodb-odm-bundle" : "^4.3",
		"guzzlehttp/guzzle" : "^7.3",
		"kreait/firebase-bundle" : "^2.4",
		"symfony/browser-kit" : "4.4.*",
		"symfony/framework-bundle" : "^4.4",
		"symfony/mailer" : "4.4.*",
		"symfony/monolog-bundle" : "^3.7",
		"symfony/orm-pack" : "^2.1",
		"symfony/phpunit-bridge" : "^4.4"
	},
	"suggest" : {
		"coka/pagination-bundle" : "^5.8",
		"doctrine/mongodb-odm-bundle" : "^4.3",
		"kreait/firebase-bundle" : "^2.4",
		"symfony/mailer" : "4.4.*",
		"symfony/orm-pack" : "^2.1"
	},
	"config" : {
		"sort-packages" : true
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/notifier-server-bundle/blob/master/docs/index.md"
	}
}
